{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "reporters": [
    "html",
    "clear-text",
    "progress"
  ],
  "testRunner": "jest",
  "coverageAnalysis": "perTest",
  "jest": {
    "configFile": "jest.config.cjs"
  },
  "mutate": [
    "src/**/*.js",
    "!src/**/*.test.js",
    "!src/**/*.spec.js",
    "!src/examples/**"
  ],
  "testMatch": [
    "tests/**/*.test.js",
    "tests/**/*.spec.js"
  ],
  "concurrency": 1,
  "timeoutMS": 2000,
  "maxTestRunnerReuse": 0,
  "ignoreStatic": true,
  "warnings": {
    "slow": false
  },
  "dashboard": {
    "project": "github.com/Syntropysoft/syntropyfront",
    "version": "main"
  }
} 