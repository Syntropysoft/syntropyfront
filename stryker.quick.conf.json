{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "pnpm",
  "reporters": [
    "html",
    "clear-text",
    "progress"
  ],
  "testRunner": "jest",
  "coverageAnalysis": "perTest",
  "plugins": [
    "@stryker-mutator/jest-runner"
  ],
  "jest": {
    "configFile": "jest.config.cjs"
  },
  "mutate": [
    "src/**/*.js",
    "!src/**/*.test.js",
    "!src/**/*.spec.js",
    "!src/examples/**"
  ],
  "concurrency": 1,
  "timeoutMS": 5000,
  "ignoreStatic": true,
  "warnings": {
    "slow": false
  }
}