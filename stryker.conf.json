{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "pnpm",
  "reporters": [
    "html",
    "clear-text",
    "progress"
  ],
  "testRunner": "jest",
  "coverageAnalysis": "perTest",
  "plugins": [
    "@stryker-mutator/jest-runner"
  ],
  "jest": {
    "configFile": "jest.config.cjs",
    "projectType": "custom",
    "config": {
      "testMatch": [
        "**/tests/*.test.js",
        "!**/tests/agent.test.js",
        "!**/tests/persistentBufferManager.test.js"
      ]
    }
  },
  "mutate": [
    "src/**/*.js",
    "!src/**/*.test.js",
    "!src/**/*.spec.js",
    "!src/examples/**"
  ],
  "ignorePatterns": [
    "node_modules/**/*",
    "dist/**/*",
    "coverage/**/*",
    "examples/**/*"
  ],
  "thresholds": {
    "high": 70,
    "low": 40,
    "break": null
  },
  "timeoutMS": 10000,
  "concurrency": 2
}